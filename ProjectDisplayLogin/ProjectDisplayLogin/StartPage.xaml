<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectDisplayLogin.StartPage">
    <ContentPage.Content>
        <StackLayout Background="White">
            <Label Text = "Login" FontSize="Large" />
            <Entry x:Name = "LoginEntry"></Entry>
            <Label Text = "Password" FontSize = "Large" />
            <Entry x:Name = "PasswordEntry"></Entry>
            <Button Text="In" Clicked="EnterButtonClicked" ></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>